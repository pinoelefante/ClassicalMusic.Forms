            xmlns:pages="clr-namespace:ClassicalMusic.Views"
            xmlns:views="clr-namespace:pinoelefante.Views"
            x:Class="ClassicalMusic.Views.MainPage"
            BindingContext="{Binding MainPageVM, Source={StaticResource Locator}}">
    
    <NavigationPage Title="Music" x:Name="navigationPage">    
        <x:Arguments>
            <pages:ComposerList Title="Compositori" />
        </x:Arguments>
    </NavigationPage>

    <pages:RadioPage />
</views:MyTabbedPage>